{
  "version": 2,
  "builds": [
    { "src": "server-with-local.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "(.*)", "dest": "server-with-local.js" }
  ]
}
{
    "version": 2,
    "builds": [
        { "src": "app.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "/" }
    ]
}